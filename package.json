{
  "name": "acquia-setup-helper",
  "description": "Helper cli tool to set up acquia cli with proper permissions",
  "version": "0.0.0",
  "author": "david ornelas",
  "bin": {
    "asu-acquia-dev-setup": "./bin/run.js"
  },
  "bugs": "https://github.com/https://github.com/davidornelas11/acquia-setup-helper/acquia-setup-helper/issues",
  "dependencies": {
    "@inquirer/prompts": "^7.5.1",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "chalk": "^5.4.1",
    "inquirer": "^12.6.1",
    "js-yaml": "^4.1.0",
    "terminal-image": "^3.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json",
    "assets"
  ],
  "homepage": "https://github.com/https://github.com/davidornelas11/acquia-setup-helper/acquia-setup-helper",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "asu-acquia-dev-setup",
    "dirname": "asu-acquia-dev-setup",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "https://github.com/davidornelas11/acquia-setup-helper/acquia-setup-helper",
  "scripts": {
    "build": "shx rm -rf dist tsconfig.tsbuildinfo && tsc -b",
    "build:force": "shx rm -rf dist tsconfig.tsbuildinfo && tsc -b --force",
    "build:clean": "shx rm -rf dist tsconfig.tsbuildinfo node_modules/.cache && tsc -b",
    "dev": "npm run build && ./bin/dev.js",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
